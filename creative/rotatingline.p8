function _draw()
 t+=1
 cls(1)
 for i=0,10,3 do
  local r = (sin(t*0.01)*5)+20
  local x1 = 64+sin(t*0.01)*r
  local y1 = 64+cos(t*0.01)*r
  local x2 = 64-sin(t*0.01)*r
  local y2 = 64-cos(t*0.01)*r
  line(x1,y1,x2,y2,15)
 end
end
